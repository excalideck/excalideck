{
  "name": "@excalideck/excalideck-editor",
  "version": "0.0.0",
  "source": "src/index.ts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "clean": "rm -rf build dist",
    "build": "yarn clean && tsc && parcel build --no-autoinstall",
